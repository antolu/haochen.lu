# Database Configuration
POSTGRES_DB=photography
POSTGRES_USER=postgres
POSTGRES_PASSWORD=secure_password_change_this

# Redis Configuration (using defaults)
REDIS_URL=redis://redis:6379/0

# Backend Configuration
SECRET_KEY=your-very-secure-secret-key-change-this-in-production-use-64-chars
ADMIN_PASSWORD=secure_admin_password_change_this

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost,https://localhost,http://localhost:3000

# File Upload Configuration
MAX_FILE_SIZE=52428800
UPLOAD_DIR=uploads
COMPRESSED_DIR=compressed

# Image Processing
WEBP_QUALITY=85
THUMBNAIL_SIZE=400

# Development/Production
NODE_ENV=production
FRONTEND_URL=http://localhost
BACKEND_URL=http://localhost:8000